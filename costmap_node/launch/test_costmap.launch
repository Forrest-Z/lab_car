<launch>
  <!-- 仿真需要1、仿真模型 2、地图 3、rviz 4、tf（odom to map） 5、小车控制（planning&path_follow) -->
  <!-- run simulation -->
  <!-- 这个是gazebo模型 -->
  <!-- <include file="$(find simulation)/launch/worlds.launch"/> -->
  <param name="use_sim_time" value="true"/>
  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find simulation)/worlds/robot2020.world" respawn="false" >
      <!-- <remap from="cmd_vel" to="cmd_vel_acc" /> -->
      <param name="base_watchdog_timeout" value="0.01" />
  </node>
  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find simulation)/launch/planning_map.yaml" respawn="false" />
  <node pkg="service_node"  name="global_planning"  type="global_planning" output="screen"/>
  <!-- <node pkg="service_node"  name="test_global_planning"  type="test_global_planning" output="screen"/> -->
  <!-- 测试用tf -->
  <node pkg="tf" type="static_transform_publisher" name="map_to_base_link"
  args="0 0 0 0 0 0 map odom 100" />
  <!-- 车id -->
  <param name="car_id" 	  type="int" 			             value="0"                 />
  <!-- 发布tf -->
  <!-- <node pkg="simulation"    type="tf_pub"             name="tf_pub"             /> -->
  <!-- task node -->
  <node pkg="task_node"     type="build_up_task"      name="build_up_node"   output="screen"   />
  <!-- local plan node -->
  <node pkg="service_node"  type="local_planner_node" name="local_planner_node" />
  <!-- Run the velocity converter node -->
  <node pkg="service_node" type="vel_converter" name="vel_converter_node" respawn="false" />
  <!-- behavior tree -->
  <node pkg="decision_node" type="behavior_node"      name="bahevior_node"     />
  <!-- Run the rviz -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find simulation)/rviz/test_costmap.rviz" />

</launch>